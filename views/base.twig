{% block html_head_container %}
{% include 'html-header.twig' %}
    {% block head %}
    {% endblock %}
    </head>
{% endblock %}
<body class="{{body_class}}" data-template="base.twig">
  <!--wrapper for boxed version-->
  <div class="site-wrapper">
  {% block header %}
      <div class="sticky-bar"> 
        <div class="info-bar"> 
          <div class="pe-container"> 
            <div class="row-fluid">
              <div class="span5 tagline">
              </div>

              <div class="span7">
                <div>
                  <div class="email">
                    <i class="icon-pencil"></i>
                    <a href="mailto:branco@brancoproducoes.com" data-rel="tooltip" data-position="bottom" data-original-title="envie um email" >branco@brancoproducoes.com</a>
                  </div>
                  <div class="phone">
                    <i class="icon-phone"></i>
                    <a href="skype:+555132314142" data-rel="tooltip" data-position="bottom" data-original-title="ou nos ligue" >+55 51 3231 4142</a>
                  </div>
                  <div class="sm-icon-wrap">
                    <a href="https://twitter.com/brancoproducoes" data-rel="tooltip" data-position="bottom" data-original-title="Twitter"><i class="icon-twitter-circled"></i></a>
                    <a href="https://pt-br.facebook.com/branco.producoes.5" data-rel="tooltip" data-position="bottom" data-original-title="Facebook"><i class="icon-facebook-circled"></i></a>
                  </div>
                </div>
              </div>
            </div>
          </div><!--end container-->  
        </div><!--end infobar-->  
        <!--wide wrapper-->
        <div class="menu-bar"> 
          <div class="pe-container"> 
            <header class="row-fluid">
              <!--small logo-->
              <div class="logo span4">
                <a href="{{site.url}}" title="{{site.name}}" ><img src="{{theme.link}}/img/content/logo_head.png" alt="logo" /></a>
              </div>
              <div class="menu-wrap span8">
                <!--main navigation-->
                <!--main menu-->
                <div class="mainNav clearfix">                          
                  <!--main nav-->
                  <ul class="nav" id="navigation">
                    {% for item in menu.items %}
                      <li id="menu-item-{{item.ID}}" class="menu-item{% if item.get_children %} dropdown{% endif %} menu-item-type-{{item.type}} menu-item-object-{{item.object}} menu-item-{{item.ID}}">
                        <a href="{{item.link}}">{{item.title}}</a>
                        {% if item.get_children %}
                          <ul class="dropdown-menu">
                            {% for child in item.get_children %}
                              <li class="menu-item nav-drop-item"><a href="{{child.get_link}}">{{child.title}}</a></li>
                            {% endfor %}
                          </ul>
                        {% endif %}
                      </li>
                    {% endfor %}
                  </ul>
                </div>
                <!--drop navigation-->
                <div id="drop-nav" class="mobile-nav" data-label="Menu..."></div>
              </div>
            </header><!-- end header  -->
          </div><!--end container-->  
        </div> <!--end top bar-->
      </div><!--end sticky bar-->
  {% endblock %}
  <div class="site-body">
    <section id="content" role="main" class="content-wrapper">
      <div class="wrapper {{sidebar_class}}">
        {% block content %}
            Sorry, no content
        {% endblock %}
      </div>
    </section>

    {% block footer %}
      {% include 'footer.twig' %}
      {{ function('wp_footer') }}
    {% endblock %}
  </div><!-- end site wrapper -->
  <!-- custom compressed javascript -->
  </div>
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
    ga('create', 'UA-50703644-1', 'agendabrancoproducoes.com.br');
    ga('send', 'pageview');
  </script>
  </body>
</html>